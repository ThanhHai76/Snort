<div class="mt-3">
    <div class="mb-3">
        <h2>Event of Snort</h2>
    </div>
    <div class="alert alert-danger" role="alert"></div>
    <div class="table-wrapper-scroll-y my-custom-scrollbar">
        <table class="table table-white table-bordered table-striped table-hover mb-0">
            <thead>
                <tr class="table-success">
                    <th>#</th>
                    <th>Cid</th>
                    <th>Signature</th>
                    <th>Timestamp</th>
                </tr>
            </thead>
            <tbody>
                <% for (var i = data.length - 1; i >= 0; i--) { %>
                <tr>
                    <td><%= data.length - i %></td>
                    <td><%= data[i].cid %> </td>
                    <td><%= data[i].signature %> </td>
                    <td><%= data[i].timestamp %> </td>
                </tr>
                <% }; %>
            </tbody>
        </table>
    </div>
</div>
<%- include('sig_attack') %>