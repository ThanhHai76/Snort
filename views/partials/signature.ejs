<div class="mt-3">
    <div class="mb-3">
        <h2>Signature of Snort</h2>
    </div>
    <div class="table-wrapper-scroll-y my-custom-scrollbar mt-2">
        <table class="table table-bordered table-striped table-hover mb-0">
            <thead>
                <tr class="table-success">
                    <th>#</th>
                    <th>Sig_id</th>
                    <th>Sig_name</th>
                    <th>Sig_sid</th>
                    <th>Sig_gid</th>
                </tr>
            </thead>
            <tbody>
                <% for (var i = sig.length - 1; i >= 0 ; i--) { %>
                <tr>
                    <td><%= sig.length - i %></td>
                    <td><%= sig[i].sig_id %> </td>
                    <td><%= sig[i].sig_name %> </td>
                    <td><%= sig[i].sig_sid %> </td>
                    <td><%= sig[i].sig_gid %> </td>
                </tr>
                <% }; %>
            </tbody>

        </table>
    </div>

</div>